<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog">
                    <resource id="Magento_Catalog::catalog_inventory">
                        <resource id="PrAnd_Vendor::vendors" title="Vendors" translate="title">
                            <resource id="PrAnd_Vendor::vendors_edit" title="Edit vendors" translate="title"/>
                            <resource id="PrAnd_Vendor::vendors_addNew" title="Create vendors" translate="title"/>
                            <resource id="PrAnd_Vendor::vendors_delete" title="Delete vendors" translate="title"/>
                            <resource id="PrAnd_Vendor::vendors_massDelete" title="Mass delete vendors" translate="title"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
